---
interface Props {
  image: {
    src: string;
    alt: string;
  };
}

const { image } = Astro.props;
---

<li class="h-full">
  <img
    src={image.src}
    alt={image.alt}
    title={image.alt}
    loading="lazy"
    class="aspect-[118_/_75] w-36 object-scale-down invert sm:w-52 dark:invert-0"
  />
</li>
