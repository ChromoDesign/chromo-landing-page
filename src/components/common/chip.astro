---
interface Props {
  label: string;
  height: number;
}

const { label, height } = Astro.props;
---

<div
  class="flex items-center rounded-full bg-white px-3 py-1 text-sm font-medium leading-5 text-black dark:bg-neutral-900 dark:font-medium dark:text-white"
  style={`height: ${height}px;`}
>
  {label}
</div>
